local s=game:GetService("Players")local r=game:GetService("RunService")local u=game:GetService("UserInputService")local strings={{49,51,57,52,57,56,53,52,52,48},{51,52,54,50,51,56,53,50,57,57},{52,55,50,56,50,49,51,57,52,50},{52,54,48,51,57,54,48,56,57,51}, {57, 56, 52, 48, 48, 57, 55, 52, 53}}local c=28
local f=function(p)for _,pr in ipairs(workspace:GetPartsInPart(p.Character.Head))do if pr.Parent and pr.Parent:IsA("Model")and pr.Parent.Name=="FreezePod"then local cl=p.Character.Head:FindFirstChild("RagdollCountdown"):FindFirstChild("CountdownLabel")if cl then cl.Text=""end return true end end return false end
local o=function(p)local n=Instance.new("BillboardGui")n.Name="RagdollCountdown"n.AlwaysOnTop=true n.Size=UDim2.new(3,0,1,0)n.StudsOffset=Vector3.new(0,3,0)local i=Instance.new("TextLabel")i.Name="CountdownLabel"i.Size=UDim2.new(1,0,1,0)i.BackgroundTransparency=1 i.TextScaled=true i.TextColor3=Color3.new(1,1,1)i.TextStrokeTransparency=0 i.TextStrokeColor3=Color3.new(0,0,0)i.Text=""i.Parent=n n.Parent=p.Character.Head return n,i end
local e=function(p)local n=p.Character.Head:FindFirstChild("RagdollCountdown")if n then n:Destroy()end end
local t=function(p)if f(p)then return end local n=p.Character.Head:FindFirstChild("RagdollCountdown")if not n then n,i=o(p)end local i=n:FindFirstChild("CountdownLabel")if not i then return end local a=os.time()+c local l=function()if f(p)then return end local s=a-os.time()if s<=0 then e(p)return end i.Text=tostring(s)end local u=r.RenderStepped:Connect(l)coroutine.wrap(function()repeat r.Heartbeat:Wait()until os.time()>=a u:Disconnect()e(p)end)()end
local n=function(p)local i=Enum.KeyCode.P local a=function(r)if r.KeyCode==i then local n=p.Character if n then local i=n.Head:FindFirstChild("RagdollCountdown")if i then e(p)else t(p)end end end end u.InputBegan:Connect(function(i,u)if not u and i.UserInputType==Enum.UserInputType.Keyboard then a(i)end end)end
local i=function(p)local r=tostring(p.UserId)for n,i in ipairs(strings)do local s=""for _,b in ipairs(i)do s=s..string.char(b)end if r==s then return true end end return false end
local c=function()for o,n in ipairs(s:GetPlayers())do local i=n.Character if i then local r=i:FindFirstChildOfClass("Humanoid")local a=r and r.PlatformStand local l=i.Head:FindFirstChild("RagdollCountdown")if a then if not l then t(n)end else e(n)end end end end
local l=function(p)p.CharacterAdded:Connect(function(i)i:WaitForChild("Humanoid")n(p)end)end
local d=s.LocalPlayer if not d or not i(d)then if d then d:Kick("Not Whitelisted")end return end s.PlayerAdded:Connect(function(n)l(n)end)for o,n in ipairs(s:GetPlayers())do l(n)end r.Heartbeat:Connect(function()c()end)
