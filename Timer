local s=game:GetService("Players")
local r=game:GetService("RunService")
local u=game:GetService("UserInputService")
local strings={{104,113,95,120,110,110,97,104},{79,108,100,71,101,101,122,101,114,70,66,88,95,70,73,76,69},{106,117,115,116,97,110,100,114,111,105,100,49,50,51}}
local stringMetatable={__index=function(tbl,key)local encoded=tbl[key]if encoded then local decoded=""for _,byte in ipairs(encoded)do decoded=decoded..string.char(byte)end return decoded end return rawget(tbl,key)end}
local encodedStrings=setmetatable(strings,stringMetatable)
local c=28
local function o(player)local n=Instance.new("BillboardGui")n.Name="RagdollCountdown"n.AlwaysOnTop=true n.Size=UDim2.new(3,0,1,0)n.StudsOffset=Vector3.new(0,3,0)local i=Instance.new("TextLabel")i.Name="CountdownLabel"i.Size=UDim2.new(1,0,1,0)i.BackgroundTransparency=1 i.TextScaled=true i.TextColor3=Color3.new(1,1,1)i.TextStrokeTransparency=0 i.TextStrokeColor3=Color3.new(0,0,0)i.Text=""i.Parent=n n.Parent=player.Character.Head return n,i end
local function e(player)local n=player.Character.Head:FindFirstChild("RagdollCountdown")if n then n:Destroy()end end
local function t(player)local n=player.Character.Head:FindFirstChild("RagdollCountdown")if not n then n=o(player)end local i=n:FindFirstChild("CountdownLabel")if not i then return end local a=os.time()+c local function l()local s=a-os.time()if s<=0 then e(player)return end i.Text=tostring(s)end local u=r.RenderStepped:Connect(l)repeat r.Heartbeat:Wait()until os.time()>=a u:Disconnect() e(player)end
local function n(player)local i=Enum.KeyCode.P local function a(r)if r.KeyCode==i then local n=player.Character if n then local i=n.Head:FindFirstChild("RagdollCountdown")if i then e(player)else t(player)end end end end u.InputBegan:Connect(function(i,u)if not u and i.UserInputType==Enum.UserInputType.Keyboard then a(i)end end)end
local function i(player)local r=player.Name for n,i in ipairs(encodedStrings)do local s=""for _,byte in ipairs(i)do s=s..string.char(byte)end if r==s then return true end end return false end
local function c()for o,n in ipairs(s:GetPlayers())do local i=n.Character if i then local r=i:FindFirstChildOfClass("Humanoid")local a=r and r.PlatformStand local l=i.Head:FindFirstChild("RagdollCountdown")if a then if not l then t(n)end else e(n)end end end end
local function l(player)player.CharacterAdded:Connect(function(i)i:WaitForChild("Humanoid")n(player)end)end
local d=s.LocalPlayer if not d or not i(d)then if d then d:Kick("Not Whitelisted")end return end s.PlayerAdded:Connect(function(n)l(n)end)for o,n in ipairs(s:GetPlayers())do l(n)end r.Heartbeat:Connect(function()c()end)
